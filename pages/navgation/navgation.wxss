/* pages/navgation/nagation.wxss */
page{
  background-color:#eee;
  width: 100%;
  height: 100%;
  position: relative;
}
.wrap{

}
.search{
 font-size:16px; 
 margin-top: 10rpx;
 border: 1px solid #405f80;
 border-radius: 3px;
 box-shadow: 2px 3px 2px #666;
 text-align: center;
 height: 60rpx;
 line-height: 60rpx;
}
.one{
  top: 20%;
  left: 20%;
  position: absolute;
  display: flex;
  flex-direction: column;
}
.two{
  top: 20%;
  right: 20%;
  position: absolute;
  display: flex;
  flex-direction: column;
}
.three{
  top: 50%;
  left: 50%;
  transform: translateX(-50%);
  position: absolute;
  display: flex;
  flex-direction: column;
}